<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.PlaylistEngine">
    <method name="addToPlaylist">
      <arg name="playlistName" type="s" direction="in"/>
      <arg name="files" type="as" direction="in"/>
    </method>
    <method name="addToPlaylist">
      <arg name="playlistName" type="s" direction="in"/>
      <arg name="file" type="s" direction="in"/>
    </method>
    <method name="availablePlaylists">
      <arg type="as" direction="out"/>
    </method>
    <method name="filesInPlaylist">
      <arg type="as" direction="out"/>
      <arg name="playlistName" type="s" direction="in"/>
    </method>
    <method name="removeFromPlaylist">
      <arg name="playlistName" type="s" direction="in"/>
      <arg name="files" type="as" direction="in"/>
    </method>
    <method name="removeFromPlaylist">
      <arg name="playlistName" type="s" direction="in"/>
      <arg name="file" type="s" direction="in"/>
    </method>
    <method name="removePlaylist">
      <arg name="playlistName" type="s" direction="in"/>
    </method>
    <method name="setCurrentPlaylist">
      <arg name="playlistName" type="s" direction="in"/>
    </method>
  </interface>
</node>
